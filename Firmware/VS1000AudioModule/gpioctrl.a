
	.sect code,GPIOCtrlIdleHook	// 85 words
	.export _GPIOCtrlIdleHook
_GPIOCtrlIdleHook:	// PROLOGUE
	ldx (I6)+1,NULL	// Room for Sp and Fp
	stx MR0,(I6)+1 ; sty I7,(I6)
	stx LR0,(I6)+1 ; sty I5,(I6)	// return address
	stx I6,(I6) ; sty I4,(I6)
	ldx (I6)+1,I4	// FP' = SP, reserve space
	ldc 512,MR0
	stx A0,(I6)+1 ; sty A1,(I6)
	stx B0,(I6)+1 ; sty C0,(I6)
	stx I0,(I6)+1 ; sty I1,(I6)
	.import _uiTrigger
	ldc _uiTrigger,I0
	stx I2,(I6)+1 ; sty LC,(I6)
	stx LE,(I6) ; sty LS,(I6)

