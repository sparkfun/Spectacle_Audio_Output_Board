	.export _SpiWaitStatus
_SpiWaitStatus:	// PROLOGUE
	ldc 47,A0
	ldx (I6)+1,NULL	// Room for Sp and Fp
	stx MR0,(I6)+1 ; sty I7,(I6)
	stx LR0,(I6)+1 ; sty I5,(I6)	// return address
	stx I6,(I6) ; sty I4,(I6)
	ldx (I6)+2,I4	// FP' = SP, reserve space
	ldc 512,MR0
	stx A1,(I6)+1 ; sty I0,(I6)
	ldc 32768,A1
	ldc 49256,I0
	stx I1,(I6)+1

